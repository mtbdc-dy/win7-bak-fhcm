dojo.addOnLoad( function(){
	

	if( dojo.query("[name=strStartNode]")[0].value!=="" ){
		/*当前为追加审核环节情况*/
		fnSetFieldValue("strDenyType","2")
	}else{
		//是否有驳回后再次提交路径的默认值设置
		if(dojo.byId("key_DenySubmit") &amp;&amp; dojo.byId("key_DenySubmit").value=="1"){
			if(dojo.byId("strDenySubmit")){
				var strDenySubmit = dojo.byId("strDenySubmit").value;
				var strDenyType = "3";
				if(strDenySubmit == "0"){
					strDenyType = "1";
				}else if(strDenySubmit == "1"){
					strDenyType = "2";
				}else if(strDenySubmit == "2"){
					strDenyType = "3";
				}
				fnSetFieldValue("strDenyType",strDenyType);
				if(dojo.byId("strDenySubmitEdit") &amp;&amp; dojo.byId("strDenySubmitEdit").value == "no"){
					dojo.query('[name=strDenyType]').forEach(function(item){
						item.setAttribute("disabled",true);
					});
				}
			}
		}
	}
	
	fnChangeDenyType();
	
})
function fnChangeDenyType(){
	var grid = dijit.byId("grid");
	var jsonDisp = dojo.fromJson(grid.datastore.defaultValue );
	
	if( fnGetFieldValue("strDenyType")!=="2"){
		jsonDisp.items = dojo.filter( jsonDisp.items,function(item){
			return item.inconfigscope ;
		});
	}
	
	grid.set("context",	dojo.toJson(jsonDisp));	
	grid.render();
	
	smartdot.Validator.cache = new dojo.store.Memory({data:[], idProperty: "name"})
	
	if(jsonDisp.items.length&gt;0){
		grid.domNode.style.display="";
		dojo.query(".btn-action-key")[0].style.display="";
		dojo.byId("emptyinfo").style.display="none";
	}else{
		grid.domNode.style.display="none";
		dojo.query(".btn-action-key")[0].style.display="none";
		dojo.byId("emptyinfo").style.display="";
	}
	
	if( fnGetFieldValue("strDenyType")!=="2"){
		dojo.forEach( dojo.query("#grid table")[0].rows,function(item,index){
			if( index%2!==0 || index===0 ){
				item.cells[item.cells.length-2].style.cssText = "display:none";
				item.cells[item.cells.length-1].style.cssText = "display:none";
			}			
		} )
	}
	if( dojo.query("[name=strStartNode]")[0].value==="" ){
		if( dojo.query('[name=denyto]').length===1 ){
			dojo.query('[name=denyto]')[0].checked = true;
			fnRowClick( "denyto" );
		}
	}
	init();
	if((typeof $$Dialog) != "undefined"){
		$$Dialog._resizeFrame();
	}
}

function fnUserClick( index ){
	var aryDenyTo = fnGetFieldValue("denyto").split(",");
	var arySubmitTo = fnGetFieldValue("submitto").split(",");
	var aryNotifyTo = fnGetFieldValue("notifyto").split(",");
	
	var aryRequire = aryDenyTo.concat(arySubmitTo,aryNotifyTo)
	
	if( fnGetFieldValue("user"+index)==="" ){
		if( smartdot.Validator.getItem("user"+index) ){
			smartdot.Validator.validate("user"+index);
		}
		setTimeout(function(){
			if((typeof $$Dialog) != "undefined"){
				$$Dialog._resizeFrame();
			}
		})
	}
}

function fnRowClick( strClickType ){
	var i ;
	var strSelectType ;
	var objCheck;
	var objTrs = dojo.query("tr", dojo.query(".grid-table")[0]);
	if( dojo.query("[name=strStartNode]")[0].value==="" ){
		strSelectType = "deny" ;
	}else{
		strSelectType = "addsubmit"
	}
	if( strClickType==="denyto"){
		var grid = dijit.byId("grid");
		
		dojo.forEach( grid.rows.reverse(),function(item,index){
			var objTr = item;
			var objDenyTo = dojo.query('[name=denyto]',objTr)[0];
			
			if( typeof objCheck ==="undefined" ){
				dojo.query('[name=submitto]',objTr).attr('disabled', true);
				dojo.query('[name=notifyto]',objTr).attr('disabled', true);
				dojo.query('[name=submitto]',objTr).attr('checked', false);
				dojo.query('[name=notifyto]',objTr).attr('checked', false);
			}else{
				dojo.query('[name=submitto]',objTr).attr('disabled', false);
				dojo.query('[name=notifyto]',objTr).attr('disabled', false);
			}
			if(objDenyTo &amp;&amp; objDenyTo.checked){
				objCheck = objDenyTo;	
				objTrs[objTr.rowIndex+1].style.cssText = "";			
			}else{
				objTrs[objTr.rowIndex+1].style.cssText = "display:none";	
			}		
			
		});
		grid.rows.reverse();
	}else if( strClickType==="submitto"){
		dojo.forEach( dojo.query("[name=submitto]").reverse(),function(item,index){
			var objTr = fnGetRow( item );	
				
			if(item.checked){
				if( typeof objCheck ==="undefined" ){
					objCheck = item;
					if( strSelectType==="addsubmit" ){
						objTrs[objTr.rowIndex+1].style.cssText = "";	
					}
				}						
			}
			
			if( strSelectType==="addsubmit" &amp;&amp; objCheck != item){
				objTrs[objTr.rowIndex+1].style.cssText = "display:none";	
			}
			
			if( item.checked ){
				dojo.query('[name=notifyto]',objTr).attr('checked', false);
			}
				
			
		});
	}else if( strClickType==="notifyto"){
		dojo.forEach( dojo.query("[name=notifyto]"),function(item,index){
			var objTr = fnGetRow( item );			
			if(item.checked){
				dojo.query('[name=submitto]',objTr).attr('checked', false);					
			}				
		});
		if( strSelectType==="addsubmit"){
			fnRowClick("submitto")	
		}	
	}
	var strDenyTo = "";
	if(dojo.query("input[name='denyto']")[0]){
		strDenyTo = fnGetFieldValue("denyto");
	}
	var aryDenyTo = strDenyTo.split(",");
	var arySubmitTo = fnGetFieldValue("submitto").split(",");
	var aryNotifyTo = fnGetFieldValue("notifyto").split(",");
	
	var aryRequire = aryDenyTo.concat(arySubmitTo,aryNotifyTo)
	
	var grid = dijit.byId("grid");
	var objJson = dojo.fromJson(grid.datastore.value );
	
	dojo.forEach( objJson.items,function(item,index){
		index = (index+1).toString()
		if( dojo.query("[name=user"+index+"]") &amp;&amp; dojo.query("[name=user"+index+"]").length!=0){
			if( dojo.indexOf(aryRequire,index.toString())!==-1){
				if( smartdot.Validator.getItem("user"+index) ){
				}else{
					dojo.forEach(dojo.query("[name=user"+index+"]"),function(oRequire){
						smartdot.Validator.addItem( oRequire );
					})
				}
				smartdot.Validator.getItem("user"+index).set("required", true)
			}else{
				if( smartdot.Validator.getItem("user"+index) ){
					smartdot.Validator.getItem("user"+index).set("required", false)
				}
			}
		}
		
	})
	
	if((typeof $$Dialog) != "undefined"){
		$$Dialog._resizeFrame();
	}
}
function fnGetRow( obj ){
	while( obj.tagName.toLowerCase()!=="tr"){
		obj = obj.parentNode;
	}
	return obj;
}

function CancelClick() {	
	if((typeof $$Dialog) != "undefined"){
		$$Dialog.postMessage("cancel");
	}
}

function OKClick( objBtnSrc ){
	
	if( !smartdot.Validator.validateAll()){
		setTimeout(function(){
			if((typeof $$Dialog) != "undefined"){
				$$Dialog._resizeFrame();
			}
		})
    		return false;
	}
	
	var grid = dijit.byId("grid");
	var objJson = dojo.fromJson(grid.datastore.value );
	var aryDenyTo = [];
	
	if( dojo.query("[name=strStartNode]")[0].value==="" ){
		if( fnGetFieldValue("denyto")==="" ) { 
			alert( smartdot.i18n.getString("indiplatform.tylc.qxzbhhj","请选择驳回环节") )
			return false; 
		}
		objJson.type = fnGetFieldValue("strDenyType");
		aryDenyTo = fnGetFieldValue("denyto").split(",");
	}else{
		if( fnGetFieldValue("submitto")==="" ) { 
			alert( smartdot.i18n.getString("indiplatform.tylc.qxzzjshhj","请选择追加审核环节") )
			return false; 
		}
	}
	
	
	var arySubmitTo = fnGetFieldValue("submitto").split(",");
	var aryNotifyTo = fnGetFieldValue("notifyto").split(",");

	dojo.forEach( objJson.items,function(item,index){
		index = (index+1).toString()
		if ( dojo.indexOf(aryDenyTo,index)!==-1 ){
			item.type = "deny"
			if( dojo.query("[name=strSpgz"+index+"]")[0] )
				item.strSpgz = fnGetFieldValue("strSpgz"+index)
			if( dojo.query("[name=fldBj"+index+"]")[0] )
				item.tmpeditable = fnGetFieldValue("fldBj"+index)
			if( dojo.query("[name=fldBjIdx"+index+"]")[0] )
				item.tmpidxeditable = fnGetFieldValue("fldBjIdx"+index)
			if( dojo.query("[name=fldNewIdx"+index+"]")[0] )
				item.tmpidxnew = fnGetFieldValue("fldNewIdx"+index)
			if( dojo.query("[name=fldNotifyOptions"+index+"]")[0] ){
				objJson.notify = fnGetFieldValue("fldNotifyOptions"+index).split(",")
			}else{
				objJson.notify = "".split(",")
			}
		}else if(dojo.indexOf(arySubmitTo,index)!==-1){
			if( dojo.query("[name=strStartNode]")[0].value!=="" ){
				if( dojo.query("[name=fldNotifyOptions"+index+"]")[0] ){
					objJson.notify = fnGetFieldValue("fldNotifyOptions"+index).split(",")
				}else{
					objJson.notify = "".split(",")
				}
			}
			item.type = "submit";
		}else if(dojo.indexOf(aryNotifyTo,index)!==-1){
			item.type = "notify";
		}else{
			item.type = "";
		}
		
		if( dojo.query("[name=user"+index+"]")&amp;&amp;dojo.query("[name=user"+index+"]").length!==0 ){
			item.user = dojo.filter(item.user,function(oUser){
				return dojo.indexOf(fnGetFieldValue("user"+index).split(","),oUser.name)!==-1
			})
		}
		
	});
	var checkJson = {};
	if(window.parent){
		//wusn 2016年4月5日 驳回时校验委托  for bug 12703
		checkJson.action = "deny";	//操作类型
		var strUserCheck = "";
		for(var numData = 0;numData&lt;objJson.items.length;numData++){
			if(objJson.items[numData].type=="deny"){
				checkJson.hjcode = objJson.items[numData].id;	//环节编号
				checkJson.hjforbidwt =  objJson.items[numData].hjforbidwt;
				for(varUser = 0;varUser&lt;objJson.items[numData].user.length;varUser++){
					if(strUserCheck==""){
						strUserCheck = objJson.items[numData].user[varUser].name;
					}else{
						strUserCheck = strUserCheck+","+objJson.items[numData].user[varUser].name;
					}
				}
			}
						
		}
		checkJson.user = strUserCheck; //检测用户
		if(window.parent.dojo.query('[name=dbPath]')[0]&amp;&amp;window.parent.dojo.query('[name=dbPath]')[0].value){
			checkJson.dbpath = window.parent.dojo.query('[name=dbPath]')[0].value;	//数据库路径
		}
		if(window.parent.dojo.query('[name=flowid]')[0]&amp;&amp;window.parent.dojo.query('[name=flowid]')[0].value){
			checkJson.flowid = window.parent.dojo.query('[name=flowid]')[0].value;	//流程UNID
		}
		if(window.parent.dojo.query("input[name='flowRefKey']")[0]&amp;&amp;window.parent.dojo.query("input[name='flowRefKey']")[0].value){
			checkJson.flowrefkey = window.parent.dojo.query("input[name='flowRefKey']")[0].value;
		}
		checkJson.checkwt = "1";	//检测委托
		checkJson.clredit = "1";	//默认处理人可编辑
		if(window.parent.document.forms[0].fldEditable &amp;&amp; window.parent.document.forms[0].fldEditable.value == "false"){
			checkJson.clredit = "0";
		}
		//审批规则
		if(window.parent.dojo.query('[name=fldDrspgz]')[0]&amp;&amp;window.parent.dojo.query('[name=fldDrspgz]')[0].value){
			checkJson.spgz = window.parent.dojo.query('[name=fldDrspgz]')[0].value;
		}
		if(dojo.query('[name=strMainDocUNID]')[0] &amp;&amp;dojo.query('[name=strMainDocUNID]')[0].value){
			checkJson.unid =dojo.query('[name=strMainDocUNID]')[0].value;
		}
				 	
				 	
	}
	fnCheckUserBeforeSubmit(checkJson).then(function(){
		if((typeof $$Dialog) != "undefined"){	
			$$Dialog.postMessage(objJson)
		};
	},function(error){
		dijit.byId("OK").cancel();
	});
}
function fnGetFlowNodeI18nName(nodeid,nodename){
	var param = {};
	param.nodeid = nodeid;
	param.nodename = nodename;
	param.flowid = document.forms[0].FlowDocId.value;
	return flowinfoi18nUtil.fnGetFlowNodeI18nName(param);	
	
