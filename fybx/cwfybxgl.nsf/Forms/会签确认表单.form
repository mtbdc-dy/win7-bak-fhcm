>smartdot.i18n.requireLocalization("indiplatform.tylc");
smartdot.i18n.requireLocalization("indiplatform.oaresource");

var g_strUserTo = "";
var g_hqShowtzfs = "";	//会签确认表单通知方式是否显示

function OKClick(){
	var objDep = dojo.query("[name=strDepID]")[0];
	var objUser = dojo.query("[name=nmUser]")[0];
	var objUserMail = dojo.query("[name=fldMailSend]")[0];
	var objUserSms = dojo.query("[name=fldSms]")[0];
	var strNotify = dojo.query("[name=strNotify]")[0].value;
	var objUserMail_ryhq = dojo.query("[name=fldMailSend_ryhq]")[0];
	var objUserSms_ryhq = dojo.query("[name=fldSms_ryhq]")[0];
	if( objDep.value==="" &amp;&amp; objUser.value==="" ){
		alert(smartdot.i18n.getString("indiplatform.tylc.qxzhqrybm","请选择"+document.forms[0].fldStartDisName.value+"人员或部门"));
		return false;
	}
	
	if(g_hqShowtzfs=="yes"){
		//会签表单显示通知方式，从表单中获取通知方式   
		if(dojo.query("[name=fldMailSend_Sel]")[0]){
			objUserMail.value = dojo.query("[name=fldMailSend_Sel]")[0].checked ? "yes":"no";   		
		}
		if(dojo.query("[name=fldSms_Sel]")[0]){
			objUserSms.value = dojo.query("[name=fldSms_Sel]")[0].checked ? "yes":"no"; 
		}
	}
	
	var data = new Object();
	data.dep = getPostData().data;
	data.user = objUser.value;
	data.sendmail = objUserMail.value;
	data.sendsms = objUserSms.value;
	data.sendmail_ryhq = objUserMail_ryhq.value;
	data.sendsms_ryhq = objUserSms_ryhq.value;
	data.huiqiantype = document.getElementById("fldHuiQianType").value;
	
	if(typeof(fnHQGTSelectConfirm_custom)=="function"){
		fnHQGTSelectConfirm_custom(data);
	}
	
	//委托检测（会签部门）
	if(!fnCheckDbStatus_hqdep(objDep.value)){
		return false;
	}
	//委托检测（会签人员）
	fnCheckDbStatus_hq(objUser.value);
	
	if(g_strUserTo!=""){
		data.user = g_strUserTo;		//赋值为真实处理人，用于fnHQGTCheck判断
	}
	if( parent.fnHQGTCheck(data)==false ){
		return false;
	}
	data.user = objUser.value;	//判断完成后，处理人赋值为选择的人员（代理中会对选择的人员再次获取被委托人）
	if ($$Dialog) {
		$$Dialog.postMessage(data);
	}

}
function CancelClick() {	
	$$Dialog.postMessage("cancel");
}
	
function fnCheckDbStatus_hq(strUser) {
	g_strUserTo = "";
	//获取会签人员有委托或状态异常的人员
	var dataResult = fnGetUserTo(strUser);
	if(dataResult &amp;&amp; dataResult.userinfo){
		var userObj = dataResult.userinfo;
		var arrUserTo = canonicalizeToAbbreviates(strUser).split(",");
		var blnflag = false;
		for(var i=0;i&lt;userObj.length;i++){
			var tmpUser = "";
			for(var tmp in userObj[i]){
				tmpUser = tmp;
				break;	//只需要获取第一个key
			}
			tmpuser = canonicalizeToAbbreviates(tmpUser);
			var tmpuserto = "";
			if(userObj[i][tmpuser].bwtr){
				//有委托
				tmpuserto = canonicalizeToAbbreviates(userObj[i][tmpuser].bwtr.name);
			}
			if(tmpuserto){
				//有委托，处理人替换成被委托人
				var ind = arrayGetIndex(arrUserTo,tmpuser);
				if(ind!=-1){
					blnflag = true;
					arrUserTo[ind] = tmpuserto;
				}
			}
		}
		if (blnflag) {
			g_strUserTo = arrUserTo.join(",");
		}
	}
}

function fnGetUserTo(strUser){
	var strDbpath = "";
	var strflowname = "";
	var hjcode = "";
	var unid = "";
	var req = null;
	if(window.parent){
		if(window.parent.document.getElementsByName("dbPath_forJS")[0]){
			strDbpath = window.parent.document.getElementsByName("dbPath_forJS")[0].value;
		}
		if(window.parent.flow_branch){
			strflowname = window.parent.flow_branch.jsonInfor.flowrefkey;
		}
		if(window.parent.document.getElementsByName("curflownum_forJS")[0]){
			hjcode = window.parent.document.getElementsByName("curflownum_forJS")[0].value;
		}
		if(dojo.query("input[name='docUnid_forJS']",window.parent.document)[0]){
			unid = dojo.query("input[name='docUnid_forJS']",window.parent.document)[0].value;
		}
	}
	var strUrl = "/"+strDbpath+"/agtCheckUser?open";
	var param = {
			'action': 'ryhq',
			'user': strUser,
			'dbname': strDbpath,
			'flowname': strflowname,
			'hjcode': hjcode,
			'unid': unid,
			'returnuser': '1'
	}
	dojo.xhrPost({
		url: strUrl,
		content: param,
		handleAs: 'json',
		sync: true,
		preventCache: true,
		load: function(dataReturn){
			req = dataReturn;
		},
		error: function(error){
			console.log(error);
		}
	});
	return req;
}

dojo.addOnLoad( function(){
	var dataParam;
	if (typeof($$Dialog) !== "undefined") {
		if ($$Dialog) {
			dataParam = $$Dialog.params;
		}
	}
	if(dojo.byId("strKeyHqConfirmShowNotifyOptions_for_JS")){
		g_hqShowtzfs = dojo.byId("strKeyHqConfirmShowNotifyOptions_for_JS").value=="1"?"yes":"no";
	}
	if(window.parent &amp;&amp; window.parent.document.forms[0].fldNiGaoDW_id_js){
		document.forms[0].fldNiGaoDW_id_js.value = window.parent.document.forms[0].fldNiGaoDW_id_js.value;
	}
 	fnReciveData(dataParam);
});
function fnSelectDept(){	
	var strUrl = "/indishare/gwpz.nsf/frmSelectDept?openform&amp;treetype="+encodeURI("会签管理")+"&amp;distzfs="+g_hqShowtzfs+"&amp;srcdb="+location.pathname;
	var strFeature = "status=yes,scrollbars=no,screenX=50,screenY=50,width="+450+",height="+425;
	window.open( strUrl,"",strFeature )
}
function getPostData( ){
	var objDepID = dojo.query("[name=strDepID]")[0];
	var objDepName = dojo.query("[name=strDep]")[0];
	
	var initvalue = objDepID.value; 
	var initname = objDepName.value;
	initvalue = initvalue.replace(/, /gi,",");
	initname = initname.replace(/, /gi,",");
	var oInitv = initvalue.split(",");
	var oInitn = initname.split(",");
	
	var dataParam = new Object();
	var data = dojo.map( oInitv, function(item,i){
		return {
			id: item,
			name: oInitn[i]
		}
	});
	dataParam.data = data
	
	if (typeof($$Dialog) !== "undefined") {
		if ($$Dialog) {
			if($$Dialog.params.huiqiantype){
				dataParam.huiqiantype = document.getElementById("fldHuiQianType").value ;				
			}
		}
	}
	
	return dataParam;
}
function fnReciveData( dataParam ){
	var objDepID = dojo.query("[name=strDepID]")[0];
	var objDepName = dojo.query("[name=strDep]")[0];
	
	if( dataParam ){
		if(g_hqShowtzfs!="yes"){
			//会签页面不显示通知方式时，通知方式从选择人员或选择部门表单中获取
			if(document.forms[0].fldMailSend.value==""){
				document.forms[0].fldMailSend.value = "no";
			}
			if(document.forms[0].fldSms.value==""){
				document.forms[0].fldSms.value = "no";
			}
			if( document.forms[0].fldMailSend_ryhq ){
				if(document.forms[0].fldMailSend_ryhq.value==""){
					document.forms[0].fldMailSend_ryhq.value = "no";
				}	
			}
			if( document.forms[0].sendsms_ryhq){
				if(document.forms[0].sendsms_ryhq.value=="" ){
					document.forms[0].sendsms_ryhq.value = "no";
				}	
			}
			
			if( dataParam.sendmail ){
					document.forms[0].fldMailSend.value = dataParam.sendmail;
			}
			if( dataParam.sendsms ){
					document.forms[0].fldSms.value = dataParam.sendsms;
			}
			if( dataParam.sendmail_ryhq ){
				if(document.forms[0].fldMailSend_ryhq){
					document.forms[0].fldMailSend_ryhq.value = dataParam.sendmail_ryhq;
				}	
			}
			if( dataParam.sendsms_ryhq ){
				if(document.forms[0].fldSms_ryhq){
					document.forms[0].fldSms_ryhq.value = dataParam.fldSms_ryhq;
				}
			}
		}
		if('struser' in dataParam){ //会签人员
			document.getElementsByName('Address')[0].value = "nmUser";
			document.getElementsByName('nmUser')[0].value = dataParam.struser;
			
			onSelect();
			return;
		}
		var strDepId = "" ;
		var strDepName = "";
		dojo.forEach(dataParam.dep,function(item){
			strDepId += (strDepId==""?"":",") + item.id ;
			strDepName += (strDepName==""?"":",") + item.name ;
		});
		objDepID.value = strDepId ;
		objDepName.value = strDepName ;
		objDepName.parentNode.setAttribute("data-department",strDepId);
		if( document.getElementsByName('Address')[0] ){
			document.getElementsByName('Address')[0].value = "strDep";
			onSelect();
		}	
		if( document.getElementById("fldHuiQianType") )
		{	
			if( dataParam.huiqiantype ){
				document.getElementById("fldHuiQianType").value= dataParam.huiqiantype;
			}
		}
		fnOnSelDep("strDep");	//部门显示多语言
	}
}
function fnSelHQry() {

	document.getElementsByName('Address')[0].value = "nmUser";
	
	var strDbpath = "";
	var flowid = "";
	var flowrefkey = "";
	var hjcode = "";
	if(window.parent){
		if(window.parent.document.getElementsByName("dbPath_forJS")[0]){
			strDbpath = window.parent.document.getElementsByName("dbPath_forJS")[0].value;
		}
		if(window.parent.flow_branch){
			flowid = window.parent.flow_branch.jsonInfor.flowunid;
			flowrefkey = window.parent.flow_branch.jsonInfor.flowrefkey;
		}else if(dojo.query("input[name='flowid']",window.parent.document)[0]){
			flowid = dojo.query("input[name='flowid']",window.parent.document)[0].value;
		}
		if(window.parent.document.getElementsByName("curflownum_forJS")[0]){
			hjcode = window.parent.document.getElementsByName("curflownum_forJS")[0].value;
		}
	}
	var uts = dojo.config.cacheBust?"&amp;"+dojo.config.cacheBust:"";
	var sSelectPersonURL = 'indishare/oaresource.nsf/frmSubmitNode?OpenForm&amp;mode=all&amp;distzfs='+g_hqShowtzfs+"&amp;docdbpath=" + strDbpath+"&amp;flowId="+flowid+"&amp;flowRefKey="+flowrefkey+"&amp;fudraftdep="+ document.forms[0].fldNiGaoDW_id_js.value+"&amp;flowNum="+hjcode+"&amp;actLb=ryhq&amp;checkUserEmpty=no&amp;readdefault=1" + uts;
    	var url = fnGetUrlRoot() +sSelectPersonURL;
    	var width = 740;
    	var height="440"; 
    	var wid="choosenew"; 
    	window.open(url,wid,'status=yes,scrollbars=no,screenX=50,screenY=50,width='+width+',height='+height);
}</javascript></code>
