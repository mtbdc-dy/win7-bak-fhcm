				If strmonth=j+1 Then
					Call jsonarryobj.Additem("strmonth",CStr(j+1))
					Call jsonarryobj.Additem("intwcszj", wczs)
					Call jsonarryobj.Additem("intybwcszj", ybwcs)
					Call jsonarryobj.Additem("intzxwcszj", zxwcs)
