//选择预付款主数据
function fnSelectview_yufukuan(db,view,flds,title,mode,value){ 
	var strdbpath = document.forms[0].dbPath_JS.value;
	db = strdbpath.substr(0,strdbpath.lastIndexOf("/"))+"/fylyfsq.nsf";
	if(dojo.byId("skdwxx").value==""){
		alert("请选择收款方户名");
		return false;
	}
	key = '[fldSkrxm_xm] ='+dojo.query("[name=skdwxx]")[0].value;
	key = encodeURIComponent(key);
	var paramobj = {
			'db':db,
			'view':view,
			'flds':flds,
			'title':title,
			'mode':'1',
			'key':key,
			'callbackFun':function(data){	
				var obj=eval(data); 
				var obj=dojo.fromJson(data);	
				var yfkunid = "";
			 	var yfkbh = "";
			 	var hxje = "0.00";
			 	for(var i = 0;i&lt;obj.length;i++){
					if(yfkbh == ""){
						yfkbh = obj[i].fldFwbh;
					}else{
						yfkbh = yfkbh + "," + obj[i].fldFwbh;
					}
					if(yfkunid == ""){
						yfkunid = obj[i].fldunid;
					}else{
						yfkunid = yfkunid + "," + obj[i].fldunid;
					}
					hxje = (parseFloat(hxje) + parseFloat(obj[i].FLDSQJE_XM)).toFixed(2);
				}
				dojo.query("[name=hxje]")[0].value = hxje;   	// 核销金额
				dojo.query("[name=glfyyf]")[0].value = yfkbh;		//关联费用预付
				dojo.query("[name=yfkhkpzunid]")[0].value = yfkunid;   	// 预付款unid
				//计算实付金额	
				fnGetSfjedt_xm(hxje);
		   }
		};

	fnSelect(paramobj);
}

