function fnGetSfjedt_xm(yfje){	
	var yfje = dojo.byId("dwyfje").value;
	if(yfje == ""){ yfje = "0.00"};
	var hxje = dojo.byId("dwhxje").value;
	if(hxje == ""){ hxje = "0.00"};
	//var sfje = (parseFloat(yfje)-parseFloat(hxje)).toFixed(2);
	//add by wx 2018-11-05增加挂账金额和支票核销金额的扣减
	var gzje = dojo.byId("dwgzje").value;
	if(gzje == ""){ gzje = "0.00"}
	var zphxje = dojo.byId("dwzphxje").value;
	if(zphxje == ""){ zphxje = "0.00"}
	var sfje = (parseFloat(yfje)-parseFloat(hxje)-parseFloat(gzje)-parseFloat(zphxje)).toFixed(2);
	dojo.byId("dwsfje").value = sfje;
	dojo.byId("dwsfje").value = parseFloat(sfje).toFixed(2);
	var tbje=parseFloat(sfje).toFixed(2);
    	if(tbje&gt;0){
     	dojo.byId("dwtbje").value="补"+tbje;
    	}else{
     	dojo.byId("dwtbje").value="退"+(0-tbje);
     }
}
