//计算实付金额
function fnGetSfje_xm(yfje){
	
	var yfje = yfje;
	var hxje = dojo.byId("fldHxje_xm").value;
	var gzje = dojo.byId("fldGzje_xm").value;
	var zpphje = dojo.byId("fldZphxje_xm").value;
	if(hxje == ""){ hxje = "0.00"}
	if(gzje == ""){ gzje = "0.00"}
	if(zpphje == ""){ zpphje = "0.00"}
	var sfje = (parseFloat(yfje)-parseFloat(hxje)-parseFloat(gzje)-parseFloat(zpphje)).toFixed(2);
	
	dojo.byId("fldSfje_xm").value = sfje;
	dojo.byId("fldYfje_xm").value = parseFloat(yfje).toFixed(2);

}
