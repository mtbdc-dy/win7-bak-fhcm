//校验起草日期
function checkenddate(){
    var date1=dojo.byId("fldKaiShiDate_xm").value;
    var date2=dojo.byId("fldJieShuDate_xm").value;
    var date1new=new Date(date1.replace(/-/g,"/"))
    var date2new=new Date(date2.replace(/-/g,"/"))
    if(date1new&gt;date2new){
       return false;
    }else{
      return true;
    }
    
}
//计算会议天数
function fnJiSunhyts(){
    var date1=dojo.byId("fldKaiShiDate_xm").value;
    var date2=dojo.byId("fldJieShuDate_xm").value;
    var date1new=new Date(date1.replace(/-/g,"/"))
    var date2new=new Date(date2.replace(/-/g,"/"))
    if(date1new&lt;=date2new){
      dojo.byId("fldhyts_xm").value=((date2new-date1new)/86400000)+1;
    }
}
//格式化金额为金额加两位小数并计算总额
function fnGeShiAndTotal(obj){
//格式化金额
if(obj.value==""){
   obj.value="0.00";
}
obj.value=parseFloat(obj.value).toFixed(2);
//计算总金额
var zsfee=dojo.byId("fldZhuSuFee_xm").value;
var czfee=dojo.byId("fldCzFee_xm").value;
var jtfee=dojo.byId("fldJiaoTongFee_xm").value;
var canfee=dojo.byId("fldCanFee_xm").value;
var zlfee=dojo.byId("fldZiLiaoFee_xm").value;
var qtfee=dojo.byId("fldQiTaFee_xm").value;
//考虑为空的情况
if(zsfee==""){
  zsfee="0.00"
}
if(czfee==""){
  czfee="0.00"
}
if(jtfee==""){
  jtfee="0.00"
}
if(canfee==""){
  canfee="0.00"
}
if(zlfee==""){
  zlfee="0.00"
}
if(qtfee==""){
  qtfee="0.00"
}
var totalfee=parseFloat(zsfee)+parseFloat(czfee)+parseFloat(jtfee)+parseFloat(canfee)+parseFloat(zlfee)+parseFloat(qtfee);
var totalfee=totalfee.toFixed(2);
dojo.byId("fldTotalFee_xm").value=totalfee;
dojo.byId("fldTotaoFeeDx_xm").value=Arabia_to_Chinese(totalfee);   
}
