function fnckSPYJ(obj){
	if(obj.checked){		
		dojo.byId("Idx_FanKui").value = dojo.byId("Idx_Tmp_Attach_5").value
	}else{
		dojo.byId("Idx_FanKui").value = ""
	}
}
	
function fnSaveDoc(strOpt){

	var def = dojo.io.iframe.send({
		form:document.forms[0],
		handleAs:'html'
	});
	def.then(function(){
		var objUserAtt = new Object();
		objUserAtt.fldseletyjform = ""; //反馈的意见附件
		objUserAtt.fldseletyj = "";     //选择的意见 
		objUserAtt.Idx_FanKui = "";     //选择意见附件 

		//反馈的意见附件
		if(dojo.query("input[name='ckAtt']"))
		{
			var obj = dojo.query("input[name='ckAtt']");

			for(var i=0;i&lt;obj.length;i++)
			{
				if(obj[i].checked)
				{
					if(objUserAtt.Idx_FanKui=="")
					{
						objUserAtt.Idx_FanKui=obj[i].value;
					}
					else
					{
						objUserAtt.Idx_FanKui+="|"+obj[i].value;
					}
				}
			}
		}
		
		//选择上流转记录意见 
		objUserAtt.fldseletyj = fnGetFieldValue("ckYjToFlow");
		
		//选择上表单的意见 
		objUserAtt.fldseletyjform = fnGetFieldValue("ckYjToForm")
		
		//选择普通附件
		objUserAtt.Idx_FanKui_Normal = [];
		dojo.forEach($$idx.containers,function(oArea){
			dojo.forEach(oArea.selectedFiles,function(funid){
				if(dojo.indexOf(objUserAtt.Idx_FanKui_Normal,funid)==-1)objUserAtt.Idx_FanKui_Normal.push(funid)
			})
		})    
		
		var dataParam;
		if (typeof($$Dialog) !== "undefined") {
			if ($$Dialog) {
				dataParam = $$Dialog.params;
			}
		}
		if( dataParam ){
			if (dataParam.isSeletYJ == "0"){
				objUserAtt.fldseletyj = dojo.byId("fldallyjcount").value;
				objUserAtt.fldseletyjform = dojo.byId("fldallyjcount").value;
			}
		}
		
		if( objUserAtt.fldseletyj==="" &amp;&amp; objUserAtt.fldseletyjform==="" ){
			alert(smartdot.i18n.getString("indiplatform.oaresource.qxzfkyj","请选择反馈意见！"));
			return false;
		}
		
		objUserAtt.fldAttitude=dojo.query("textarea[name='fldAttitude']")[0].value  // 当前用户审批意见有修改回写
		
		objUserAtt.Idx_Tmp_Attach_5 = dojo.byId("Idx_Tmp_Attach_5").value  //当前用户意见附件有修改回写
		
		if( objUserAtt.fldseletyjform == dojo.byId("fldallyjcount").value ){
			if( objUserAtt.Idx_FanKui=="" &amp;&amp; objUserAtt.fldAttitude=="" &amp;&amp; objUserAtt.Idx_Tmp_Attach_5=="" &amp;&amp;parent.WebPaintLocalPath==''){
				alert(smartdot.i18n.getString("indiplatform.oaresource.qtxfkyj","请填写反馈意见！"));
				document.forms[0].fldAttitude.focus();
				return false;
			}
		}
		
		fnBeforeHqfk().then(function(){
			$$Dialog.postMessage(objUserAtt);
		});
	});

}

//会签反馈前检测
function fnBeforeHqfk(){
	var deferred = new dojo.Deferred();
	
	//附件或意见附件上传是否完成
	var errhandle=dojo.subscribe("/attach/submiterror",function(){
		dojo.unsubscribe(errhandle);
		dojo.unsubscribe(handle);
	});
	var handle=dojo.subscribe("/wfeditor/submitover",function(){
		dojo.unsubscribe(errhandle);
		dojo.unsubscribe(handle);
		deferred.resolve("success");
	});
	UploadIndiDocFiles();
	
	return deferred.promise;
}

function fnCancle(){
	$$Dialog.postMessage("cancel");
	
}
//意见全选
function clickAll( blnAll )
{
	if( blnAll ){
		document.getElementById("selAll").style.display="none";
		document.getElementById("selNone").style.display="inline";
	}else{
		document.getElementById("selAll").style.display="inline";
		document.getElementById("selNone").style.display="none";
	}
	
	dojo.query("input[type=checkbox]").forEach(function(obj){
		if(obj.name!="chkidx_common"){
    			obj.checked = blnAll;
    		}
	});

}
//附件全选
function clickAllAtt( blnAll )
{
	if( blnAll ){
		document.getElementById("selAllAtt").style.display="none";
		document.getElementById("selNoneAtt").style.display="inline";
	}else{
		document.getElementById("selAllAtt").style.display="inline";
		document.getElementById("selNoneAtt").style.display="none";
	}
	
	dojo.query("input[type=checkbox]").forEach(function(obj){
		if(obj.name=="chkidx_common"){
    			obj.checked = blnAll;
    			if(blnAll){
    				updateSelectedIdxFiles(null,"_all",obj.value,"add")
    			}else{
    				updateSelectedIdxFiles(null,"_all",obj.value,"delete")
    			}
    		}
	});

}
//反选
function clickRevert()
{
	dojo.query("input[type=checkbox]").forEach(function(obj){
		if(obj.name!="chkidx_common"){
			if (obj.checked){
				obj.checked=false;			
			}else{
				obj.checked=true;	
			}
		}
	});

}
/*选择上表单*/
function ckform(i){
 	if (dojo.query("input[name='ckYjToForm']")[i-1].checked){
		dojo.query("input[name='ckYjToFlow']")[i-1].checked=true;
	}
	if (!dojo.query("input[name='ckYjToForm']")[i-1].checked){
		dojo.query("input[name='ckYjToFlow']")[i-1].checked=false;
	}
}

function fnParseYJRow( item,isDep,i ){
	var strHTML = "";
	var strUser = getEntryText(item.getElementsByTagName("user")[0]);
	var strAttitude = getEntryText(item.getElementsByTagName("yj")[0]);
	strAttitude = strAttitude.replace(/\n/g,"&lt;br/&gt;");
	var strStat = getEntryText(item.getElementsByTagName("stat")[0]);
	var strTime = getEntryText(item.getElementsByTagName("time")[0]);
			var strYjpt = getEntryText(item.getElementsByTagName("yjtp")[0]);
			var shenpijibie = getEntryText(item.getElementsByTagName("shenpijibie")[0]);
			var flownum = "";
			if(item.getElementsByTagName("flownum").length!=0){
				flownum = getEntryText(item.getElementsByTagName("flownum")[0]);
				strStat = parent.fnGetFlowNodeI18nName(flownum,strStat);
			}
			var strYjatt = "";
			
			if( item.getElementsByTagName("attname")[0] ){
				strYjatt = getEntryText(item.getElementsByTagName("attname")[0]);
			}
			var strSignature =  getEntryText(item.getElementsByTagName("signature")[0]);
			
			
			var YJHTML ="";
			
			if( !isDep ){
				strHTML +="&lt;tr&gt;"+"&lt;th&gt;"+(i+1)+".&lt;/th&gt;"+"&lt;th&gt;";
			}else{
				strHTML +="&lt;tr class=\"rowHQYJ\"&gt;"+"&lt;th&gt;&lt;/th&gt;"+"&lt;th&gt;";	
			}
			
		
			if( strSignature!=="" ){
				strHTML += "&lt;div class=\"sign\" id=\"user"+i+"\" value=\""+strUser+"\"&gt;"
						+"&lt;img height=\"50px\" title=\""+strUser+"\" alt=\""+strUser+"\" src=\""+strSignature+"\"&gt;:"
 						+"&lt;/div&gt;"
			
			}else{
				strHTML += "&lt;div id=\"user"+i+"\" value=\""+strUser+"\"&gt;"+ parseName(strUser) +":&lt;/div&gt;"
			}
			strHTML += "&lt;/th&gt;"
					+"&lt;td&gt;"
					
					+"&lt;div&gt;";
			if( strAttitude!=="NoAttitude_Handler" ){
				strHTML += strAttitude;
			}
			var HaHtml = ""
			if (strYjpt!==""){
				
				HaHtml += '&lt;a onclick="fnShowSxtp(\'手写批示\',\''+strYjpt+'\')"&gt;&lt;img src="/indishare/oaresource.nsf/images/tablet-icon.png" width="32" height="32" '+
				'title="点击此处查看手写批示"&gt;&lt;/a&gt;'
			}		
			strHTML += "&lt;/div&gt;";
			if( strYjatt !=="" ){
				strHTML += "&lt;div style=\"margin-top:3px;height:32px\"&gt;"
				strHTML += "&lt;img title=\""+strYjatt+"\" src=\"/indishare/oaresource.nsf/images/icon29.png\"&gt;&lt;a onclick=\"OpenMyFile('"+strYjatt+"')\"&gt;" + strYjatt + "&lt;/a&gt;"
				strHTML += HaHtml
								
				strHTML +="&lt;/div&gt;"
			}else{
				if (strYjpt!==""){
					strHTML += "&lt;div style=\"margin-top:3px\"&gt;"
					strHTML += HaHtml
					strHTML +="&lt;/div&gt;"
				}
			}
				
			strHTML += "&lt;div style=\"color:gray;height:20px\"&gt;" + strStat + "   "+ strTime + "&lt;/div&gt;"
					+"&lt;/td&gt;&lt;th&gt;&lt;label style='display:"+(document.forms[0].tmphqyjToField.value!=="yes"?"none":"")+"'&gt;&lt;input onclick='ckform("+(i+1)+")' onchange='ckform("+(i+1)+")'  name='ckYjToForm'  type='checkbox' value='"+(i+1) +"'&gt;"+smartdot.i18n.getString("indiplatform.tylc.cldyj","上审批单")+"&lt;/label&gt;&lt;label style=\"padding-left:10px;\"&gt;&lt;input name='ckYjToFlow'  type='checkbox' value='"+(i+1) +"'&gt;"+smartdot.i18n.getString("indiplatform.tylc.spyj","审批意见")+"&lt;/label&gt;&lt;/th&gt;&lt;/tr&gt;";
					
	return strHTML;
		
}
function fnGetYJCount( xml ){
	var numCount = 0 ;
	dojo.forEach( xml.getElementsByTagName("yjitem"),function(item){
		if( item.getElementsByTagName("subhqfk").length==0){
			numCount +=1;
		}else{
			numCount += item.getElementsByTagName("subhqfk").length;
		}
	})
	return numCount;
}</javascript></code>
