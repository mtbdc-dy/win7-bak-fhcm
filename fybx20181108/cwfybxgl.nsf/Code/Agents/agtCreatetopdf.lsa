Option Public
Option Declare


</lotusscript></code><code event='initialize'><lotusscript>Sub Initialize
	Dim session As New NotesSession
	Dim db As NotesDatabase
	Dim vw As NotesView
	Dim doc As NotesDocument
	Dim paramNoteId As String
	Dim docTmp As NotesDocument
	
	Set db = session.Currentdatabase
	Set vw = db.Getview("vwDocByAll")
	Set doc = vw.Getfirstdocument()
	If Not doc Is Nothing Then
		Call docTmp.Replaceitemvalue("docNoteid", doc.Noteid)
		Call docTmp.Save(True,False)
		paramNoteId=docTmp.Noteid
		
		Delete doc '删除当前过程对该文档的引用
		Call db.Getagent("agtCreatePDF2").Run(paramNoteId)
		
		Set docTmp=db.Getdocumentbyid(paramNoteId)
		Call docTmp.Remove(True)
	End If
End Sub</lotusscript></code>
