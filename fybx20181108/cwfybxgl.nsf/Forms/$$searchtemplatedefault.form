function setViewSelect() {
  if (!document.forms[0].elements["switchView"]) return;
  if (document.forms[0].elements["switchView"].length &gt; 1) {
    var viewname = document.forms[0].elements["viewname"].value.toLowerCase();
    for (var i = 0; i &lt; document.forms[0].elements["switchView"].length; i++) {
      var strValue = document.forms[0].elements["switchView"].options(i).value.toLowerCase();
      if (getRefValue(strValue, "view=") == viewname)
        document.forms[0].elements["switchView"].options(i).selected = true;
