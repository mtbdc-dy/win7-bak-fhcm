function fnOrder() {
  var varURL = window.location.href;
  var varSelectOrder;
  for (i = 0; i &lt; document.forms[0].elements["fldOrder"].length; i++) {
    if (document.forms[0].elements["fldOrder"][i].selected) {
      varSelectOrder = document.forms[0].elements["fldOrder"][i].value;
    }
  }
  varSelectOrder = "&amp;SearchOrder=" + varSelectOrder;
  if (varURL.indexOf("&amp;SearchOrder=") == -1) {
    varURL = varURL + varSelectOrder;
  } else {
    varURL = varURL.replace("&amp;SearchOrder=1", varSelectOrder);
    varURL = varURL.replace("&amp;SearchOrder=2", varSelectOrder);
    varURL = varURL.replace("&amp;SearchOrder=3", varSelectOrder);
    //    varURL = varURL.replace("&amp;SearchOrder=4",varSelectOrder);
  }


  //取&amp;start=之后的所有字符串,返回的字符如：start=21或start=21&amp;categary=sss
  var intBegin = varURL.indexOf("&amp;start=");
  if (intBegin != -1) {
    var strTemp = varURL.substring(intBegin + 1, varURL.length);
    //判断strTemp中是否有&amp;

    intBegin = strTemp.indexOf("&amp;");
    if (intBegin != -1) {
      var strTemp1 = strTemp.substring(0, intBegin);
      varURL = varURL.replace(strTemp1, "");
    } else {
      varURL = varURL.replace(strTemp, "");
    }
  }
  window.location = varURL;
}

function fnClose() {
  var pathname = (window.location.pathname);
  pathname = pathname.substring(0, (pathname.lastIndexOf('.nsf') + 5));
  var strView = getRefValue(location.href.toLowerCase(), "view=");
  if (strView != "") strView = "&amp;view=" + strView;
  var strCat = getRefValue(location.href.toLowerCase(), "restricttocategory=");
  if (strCat != "") strCat = "&amp;restricttocategory=" + strCat;
  pathname = pathname + "/myview?openform" + strView + strCat
  window.location = pathname;
}

function selectType() {
  var pathname = (window.location.pathname);
  var formname = "frmTypeSelecter"
  var width = "300";
  var height = "200";
  var wid = "selType";
  var advisewindow = window.open(pathname.substring(0, (pathname.lastIndexOf('.nsf') + 5)) + formname + '?openFORM', wid, 'status=no,resizable=no,scrollbars=yes,screenX=50,screenY=50,width=' + width + ',height=' + height);
  advisewindow.focus();
}

